void Pserarch(int* arr, int start, int end, int count){
  
}

int main() {

  long long int n, k;
  
  cin >> n >> k;
  
  long long int arr[n][n];
  
  for(int j=1;j<=n;j++){
    for(int i=1;i<=n;i++){
      arr[j-1][i-1] == i * j;
    } 
  }
  
  for(int j=0;j<n;j++){
    for(int i=0;i<n;i++)
    {
      printf("%d ", arr[j][i]);
    }
    printf("\n");
  }
  
  //int value = Pserarch(arr, 0, n*n-1, k);
